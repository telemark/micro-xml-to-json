{
  "name": "micro-xml-to-json",
  "description": "Microservice for converting xml to json",
  "version": "1.1.2",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=10.4.1"
  },
  "scripts": {
    "test": "standard && npm audit && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "start": "micro",
    "dev": "micro-dev",
    "deploy": "npm test && now && now alias",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "keywords": [
    "microservice",
    "micro",
    "sitemap",
    "xml-to-json",
    "Telemark fylkeskommune"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/telemark/micro-xml-to-json.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/micro-xml-to-json/issues"
  },
  "homepage": "https://github.com/telemark/micro-xml-to-json#readme",
  "now": {
    "alias": [
      "xmltojson.tjeneste.win"
    ],
    "type": "npm",
    "env": {
      "NODE_ENV": "production"
    },
    "engines": {
      "node": "10.4.1"
    }
  },
  "devDependencies": {
    "ava": "0.25.0",
    "coveralls": "3.0.2",
    "micro-dev": "3.0.0",
    "nyc": "13.0.1",
    "standard": "12.0.0",
    "test-listen": "1.1.0"
  },
  "dependencies": {
    "axios": "0.18.0",
    "markdown-it": "8.4.2",
    "micro": "9.3.3",
    "xml2js": "0.4.19"
  }
}
