{
  "name": "micro-xml-to-json",
  "description": "Microservice for converting xml to json",
  "version": "1.1.0",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=8.8.1"
  },
  "scripts": {
    "test": "standard && nsp check && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "start": "micro",
    "now-deploy": "npm test && now --npm && now alias"
  },
  "keywords": [
    "microservice",
    "micro",
    "sitemap",
    "Telemark fylkeskommune"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/telemark/micro-xml-to-json.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/micro-xml-to-json/issues"
  },
  "homepage": "https://github.com/telemark/micro-xml-to-json#readme",
  "now": {
    "alias": [
      "xmltojson.microservice.click"
    ],
    "type": "npm",
    "env": {
      "NODE_ENV": "production"
    }
  },
  "devDependencies": {
    "ava": "0.24.0",
    "coveralls": "3.0.0",
    "nsp": "3.1.0",
    "micro-dev": "2.1.1",
    "nyc": "11.4.1",
    "standard": "10.0.3",
    "test-listen": "1.1.0"
  },
  "dependencies": {
    "axios": "0.17.1",
    "markdown-it": "8.4.0",
    "micro": "9.0.2",
    "xml2js": "0.4.17"
  }
}
