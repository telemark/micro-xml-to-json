{
  "name": "micro-xml-to-json",
  "description": "Microservice for converting xml to json",
  "version": "1.0.7",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=7.10.0"
  },
  "scripts": {
    "test": "standard && nsp check && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "start": "micro",
    "now-deploy": "npm test && now --npm && now alias"
  },
  "keywords": [
    "microservice",
    "micro",
    "sitemap",
    "Telemark fylkeskommune"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/telemark/micro-xml-to-json.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/micro-xml-to-json/issues"
  },
  "homepage": "https://github.com/telemark/micro-xml-to-json",
  "now": {
    "alias": [
      "xmltojson.microservice.click"
    ],
    "type": "npm",
    "env": {
      "NODE_ENV": "production"
    }
  },
  "devDependencies": {
    "ava": "0.21.0",
    "coveralls": "2.13.1",
    "nsp": "2.6.3",
    "nyc": "11.0.3",
    "standard": "10.0.2",
    "test-listen": "1.0.2"
  },
  "dependencies": {
    "axios": "0.16.2",
    "marked": "0.3.6",
    "micro": "8.0.0",
    "xml2js": "0.4.17"
  }
}